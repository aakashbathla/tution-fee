{
    "presets": [
        ["env", {
            "targets": {
                "browsers": [
                    "> 1%",
                    "last 2 versions"
                ]
            }
        }],
        "stage-2",
        "react"
    ],
    "plugins": [
        "syntax-dynamic-import",
        ["transform-runtime", {
            "polyfill": false,
            "regenerator": true
          }],
		["transform-es2015-arrow-functions", { "spec": true }]
    ]
}